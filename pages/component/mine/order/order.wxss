@import "/colorui/main.wxss";
@import "/colorui/icon.wxss";

item.none{
  display: none;
}
.split{
  height: 17rpx;
}

.swiper-tab {
  line-height: 80rpx;
  border: 1px solid #ccc;
  display: flex;
  justify-content: space-around;
}

.swiper-tab-list {
  font-size: 30rpx;
  color: #777;
  text-align: center;
}

.active {
  color: #6a005f;
  border-bottom: 5rpx solid #6a005f;
}